image {
    width: 100%;
    height: 100%;
    display: block;
}

.container {
    width: 100%;
    height: 100vh;
    background-size: 100%;
    background-repeat: no-repeat;
    position: relative;
    overflow-x: hidden;
    overflow-y: auto;
    background-repeat: no-repeat;
    background-size: 100%;
    padding-bottom: 100rpx;
}

.rank {
    position: absolute;
    top: 277rpx;
    left: 50%;
    transform: translateX(-50%);
}

.rank .dbButtons {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 570rpx;
    margin: 0 auto;
    color: #fff;
    font-size: 40rpx;
}

.rank .dbButtons view {
    width: 293rpx;
    height: 84rpx;
    background-size: 100%;
    background-repeat: no-repeat;
    text-align: center;
    line-height: 63rpx;
}

.teamMain,
.personalMain {
    width: 691rpx;
    height: 775rpx;
    background-size: 100%;
    background-repeat: no-repeat;
    overflow: hidden;
}

.teamMain .nameAndRanking {
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #fff;
    width: 440rpx;
    margin: 30rpx auto 0;
}

.teamMain .teamName {
    font-size: 40rpx;
}

.teamMain .Ranking {
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
}

.teamMain .Medal {
    width: 31rpx;
    height: 45rpx;
}

.teamMain .memberBox {
    width: 575rpx;
    display: flex;
    justify-content: space-between;
    margin: 15rpx auto 0;
}

.teamMain .teamMember_item {
    width: 175rpx;
    height: 190rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background-color: #1dd0eb;
    border-radius: 10rpx;
    border: 1px solid #fff;
}

.teamMain .teamMember_item .icon {
    width: 84rpx;
    height: 84rpx;
    margin: 10rpx 0;
    border-radius: 50%;
    overflow: hidden;
}

.teamMain .teamMember_item .data {
    width: 100%;
    font-size: 24rpx;
    color: #fff;
    display: flex;
    flex: 1;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background-color: #0c4d8e;
    border-radius: 0 0 10rpx 10rpx;
}

.teamMain .teamMember_item .data>view {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align:center;
}

.teamMain .listTeam {
    width: 580rpx;
    height: 383rpx;
    margin: 60rpx auto 0;
}

.teamMain .listTeam_item {
    width: 100%;
    height: 70rpx;
    border-radius: 10rpx;
    background-color: #fff;
    margin-bottom: 10rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 30rpx 0 45rpx;
    box-sizing: border-box;
    color: #605f5f;
}

.teamMain .listTeam_item .left {
    display: flex;
    align-items: center;
}

.teamMain .listTeam_item .left .num {
    margin-right: 50rpx;
    min-width: 50rpx;
    text-align: center;
}

.teamMain .listTeam_item .left .teamName {
    font-size: 30rpx;
}

.teamMain .listTeam_item .right {
    font-size: 24rpx;
}

.teamMain .listTeam_item:nth-of-type(1) .left .num {
    font-size: 62rpx;
    color: #edc70b;
}

.teamMain .listTeam_item:nth-of-type(2) .left .num {
    font-size: 55rpx;
    color: #b1b1b1;
}

.teamMain .listTeam_item:nth-of-type(3) .left .num {
    font-size: 48rpx;
    color: #ff9266;
}

.personalMain .top {
    width: 78%;
    margin: 80rpx auto 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #fff;
    font-size: 30rpx;
    position: relative;
}

.personalMain .left .name {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: center;
    width: 180rpx;
}

.personalMain .left .personalIcon {
    width: 48rpx;
    height: 48rpx;
}

.personalMain .top .left,
.personalMain .top .right {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.personalMain .top .right .v {
    width: 47rpx;
    height: 46rpx;
}

.personalMain .center {
    width: 142rpx;
    height: 142rpx;
    position: absolute;
    top: -30rpx;
    left: 50%;
    transform: translateX(-50%);
}

.personalMain .person {
    width: 142rpx;
    height: 142rpx;
    background-size: 100%;
    background-repeat: no-repeat;
    display: flex;
    justify-content: center;
    align-items: center;
}

.personalMain .person .personInner {
    width: 110rpx;
    height: 110rpx;
    border-radius: 50%;
    overflow: hidden;
    position: relative;
}

.personalMain .person .line {
    width: 14rpx;
    height: 52rpx;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: -37rpx;
}

.personalMain .center .ranking {
    text-align: center;
    margin-top: 45rpx;
}

.personalMain .bottom {
    margin-top: 180rpx;
}

.personalMain .listTeam {
    width: 580rpx;
    height: 383rpx;
    margin: 60rpx auto 0;
}

.personalMain .listTeam_item {
    width: 100%;
    height: 70rpx;
    border-radius: 10rpx;
    background-color: #fff;
    margin-bottom: 10rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 30rpx 0 45rpx;
    box-sizing: border-box;
    color: #605f5f;
}

.personalMain .listTeam_item .left {
    display: flex;
    align-items: center;
}

.personalMain .listTeam_item .left .num {
    margin-right: 30rpx;
    min-width: 50rpx;
    text-align: center;
}

.personalMain .listTeam_item .left .teamName {
    font-size: 30rpx;
}

.personalMain .listTeam_item .right {
    font-size: 24rpx;
}

.personalMain .listTeam_item:nth-of-type(1) .left .num {
    font-size: 62rpx;
    color: #edc70b;
}

.personalMain .listTeam_item:nth-of-type(2) .left .num {
    font-size: 55rpx;
    color: #b1b1b1;
}

.personalMain .listTeam_item:nth-of-type(3) .left .num {
    font-size: 48rpx;
    color: #ff9266;
}